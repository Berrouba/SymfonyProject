{% extends 'base.html.twig' %}
  
   
{% block body %}
    


 <center> <h1>Category list</h1></center>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
            
            </tr>
        </thead>
        <tbody>
        {% for category in categories %}
            <tr>
                <td>{{ category.id }}</td>
                <td>{{ category.name }}</td>
             
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock%}









